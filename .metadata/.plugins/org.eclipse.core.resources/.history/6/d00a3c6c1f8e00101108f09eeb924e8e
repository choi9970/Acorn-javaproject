package DashBoard;

import java.util.ArrayList;

public class FileReader {
	
		public static void readFile() {
	        ArrayList<Board> list = CsvReader.readBoardList(FILE_PATH);
	        for(Board b : list) {
	            System.out.println(b.getId() + ". " + b.getTitle() + " | " + b.getContent() + " | " + b.getDate());
	        
	    }
	}
}
